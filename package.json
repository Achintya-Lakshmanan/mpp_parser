{
  "name": "mpp_parser",
  "version": "1.0.0",
  "description": "Microsoft Project (MPP/MPX) file parser for React",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "server": "node src/backend/server.js",
    "test-server": "node src/backend/test-server.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "test": "concurrently \"npm run start\" \"npm run test-server\"",
    "test:mock": "node src/mock/runMockTests.js",
    "test:unit": "jest",
    "gen:build": "rm -rf dist && tsc -p tsconfig.json && cp -R src/generators dist/ && cp -R src/utils dist/ && cp -R src/services dist/ && cp -R src/config dist/ && cp -R src/visuals dist/",
    "gen:start": "node dist/generator/index.js",
    "gen:dev": "ts-node-dev --respawn --transpile-only src/generator/index.ts",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "benchmark:gen": "node src/benchmark/templateGeneratorBenchmark.js"
  },
  "keywords": [
    "mpp",
    "mpx",
    "microsoft",
    "project",
    "parser",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "ajv": "^8.12.0",
    "axios": "^1.9.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "fs-extra": "^11.1.1",
    "jszip": "^3.10.1",
    "multer": "^1.4.5-lts.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-json": "^1.7.5",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@types/node": "^20.4.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack-dev-server": "^5.2.1"
  }
}
